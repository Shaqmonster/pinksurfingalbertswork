{
    "name": "@loginid/websdk3",
    "version": "3.3.2",
    "description": "",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "files": [
      "dist",
      "README.md",
      "LICENSE.md"
    ],
    "private": false,
    "publishConfig": {
      "access": "public"
    },
    "scripts": {
      "build": "tsc --noEmit && tsup && node ./post-def-typing.cjs",
      "release": "semantic-release",
      "delete-maps": "npm run delete-map-files && npm run delete-references-to-map-files",
      "delete-map-files": "find ./dist -name '*.map' -delete",
      "delete-references-to-map-files": "find ./dist -name '*.js' -or -name '*.mjs' -exec sed -i -e 's/sourceMappingURL=[^ ]*.map//g' {} +",
      "lint": "eslint \"src/**/*.ts?(x)\"",
      "test": "jest",
      "docs": "typedoc"
    },
    "author": "LoginID Inc.",
    "license": "ISC",
    "keywords": [
      "authentication",
      "passkey",
      "fido2",
      "biometrics",
      "webauthn",
      "passwordless"
    ],
    "config": {
      "commitizen": {
        "path": "./node_modules/cz-conventional-changelog"
      }
    },
    "devDependencies": {
      "@loginid/core": "*"
    }
  }
  